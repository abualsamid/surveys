{
  "name": "surveys",
  "version": "1.0.0",
  "description": "botengine website",
  "main": "index.js",
  "scripts": {
    "start": "env NODE_ENV=development npm run build && go run ../botengine_api/server.go",
    "build": "env NODE_ENV=production rm -rf public && webpack --config webpack.config.js --progress --colors -p",
    "watch": "env NODE_ENV=development webpack --watch --progress --colors",
    "server": "env NODE_ENV=production node server.js",
    "deploy": "env NODE_ENV=production npm run build  && scp -r public/ leye:/home/ubuntu/api && cd public && aws s3 sync --exclude '.gitignore' .  s3://leye-survey"
  },
